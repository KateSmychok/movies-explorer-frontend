{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "no-underscore-dangle": ["off", {"allow": ["_id"]}],
    "import/no-named-as-default": "off",
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-console": "off",
    "no-lonely-if": "off",
    "consistent-return": "off"
  }
}
